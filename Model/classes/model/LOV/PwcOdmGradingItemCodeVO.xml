<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PwcOdmGradingItemCodeVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.PwcOdmGradingItemCodeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
    <TransientExpression><![CDATA[adf.context.sessionScope.mfg_org_id]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[select  oola.INVENTORY_ITEM_ID
        ,msi.CONCATENATED_SEGMENTS
        ,msi.DESCRIPTION
        ,ooha.ORDER_NUMBER ORD_NO
from    oe_order_headers_all ooha
        ,oe_order_lines_all oola
        ,mtl_system_items_kfv msi
where   ooha.HEADER_ID = oola.HEADER_ID  
and     msi.INVENTORY_ITEM_ID = oola.INVENTORY_ITEM_ID
and     msi.ORGANIZATION_ID = nvl(:p_org_id,msi.ORGANIZATION_ID)
GROUP by 
        oola.INVENTORY_ITEM_ID
        ,msi.CONCATENATED_SEGMENTS
        ,msi.DESCRIPTION
        ,ooha.ORDER_NUMBER
ORDER BY 2]]>
  </SQLQuery>
