  <SQLQuery>
    <![CDATA[select  we.WIP_ENTITY_ID
        ,we.WIP_ENTITY_NAME
        ,msi.concatenated_segments
        ,msi.DESCRIPTION
        ,msi.inventory_item_id item_id
        ,wdj.ATTRIBUTE11 cust_po_number
        ,ooha.ORDER_NUMBER Sales_order_number
        ,wdj.start_quantity job_quantity
        ,SUM (DECODE (UPPER (mtt.transaction_type_name),
                      'WIP COMPLETION', mmt.transaction_quantity,
                      'WIP COMPLETION RETURN', mmt.transaction_quantity
                     )
             ) stitching_quantity
from   wip_discrete_jobs wdj
       ,wip_entities we
       ,mtl_material_transactions mmt
       ,mtl_transaction_types  mtt
       ,mtl_system_items_KFV msi
       ,oe_order_headers_all ooha 
where  we.WIP_ENTITY_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_SOURCE_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_TYPE_ID = mtt.TRANSACTION_TYPE_ID
and we.wip_entity_id in (select  nvl(job_id,0) from PWC_ODM_GRADING_TERRY_HEADERS)
and we.WIP_ENTITY_ID in (select  wdj.wip_entity_id from wip_discrete_jobs wdj where attribute_category = 'Job Details' and ATTRIBUTE13 = 'STITCH')
and wdj.status_type in (3,4)
and wdj.PRIMARY_ITEM_ID = msi.INVENTORY_ITEM_ID
and WDJ.ORGANIZATION_ID = msi.ORGANIZATION_ID
and nvl(wdj.ATTRIBUTE12,'-1') = nvl(to_char(ooha.HEADER_ID),'-1')
and nvl(wdj.ATTRIBUTE11,'-1') = nvl(:p_cust_po,nvl(wdj.ATTRIBUTE11,'-1'))
and ooha.ORDER_NUMBER = nvl(:p_sales_order,ooha.ORDER_NUMBER)
and msi.ORGANIZATION_ID = nvl(:p_org_id,msi.ORGANIZATION_ID) 
group by we.WIP_ENTITY_ID,we.WIP_ENTITY_NAME,msi.concatenated_segments,msi.DESCRIPTION ,wdj.ATTRIBUTE11 ,wdj.ATTRIBUTE12,ooha.ORDER_NUMBER,msi.inventory_item_id, wdj.start_quantity]]>
  </SQLQuery>
